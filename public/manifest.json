{
    "manifest_version": 3,
    "name": "Iframe QR Code Generator",
    "version": "1.0",
    "description": "Detects iframe URLs and generates QR codes.",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "web_accessible_resources": [
      {
        "resources": ["popup.html"],
        "matches": ["<all_urls>"]
      }
    ]
  }